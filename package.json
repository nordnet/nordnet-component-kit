{
  "name": "nordnet-component-kit",
  "version": "0.2.7",
  "description": "Custom react components used by Nordnet",
  "main": "lib/nordnet-component-kit.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "run-s docs:watch",
    "analyze": "ANALYZE=true run-s webpack",
    "clean:docs": "rimraf docs",
    "clean:lib": "rimraf lib",
    "clean:node_modules": "rimraf node_modules",
    "env:dev": "NODE_ENV=development ",
    "env:prod": "NODE_ENV=production",
    "prebuild": "run-s clean:lib",
    "build": "run-s env:prod webpack",
    "build:watch": "run-s webpack -- --watch",
    "webpack": "webpack --progress",
    "test": "NODE_ENV=test mocha --opts test/mocha.opts",
    "test:tdd": "run-s test -- --watch --watch-extensions js,jsx",
    "coverage": "nyc --extension .jsx npm test",
    "precoveralls": "run-s coverage",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src test webpack styleguide --ext .js,.jsx",
    "predocs": "run-s clean:docs",
    "docs": "run-s styleguide:build",
    "docs:watch": "run-s styleguide:server",
    "predocs:publish": "run-s docs",
    "docs:publish": "git-directory-deploy --directory docs --message 'Update docs' --branch gh-pages",
    "styleguide:build": "styleguidist build --config styleguide/config.js --colors",
    "styleguide:server": "styleguidist server --config styleguide/config.js --colors",
    "validate": "run-p lint test",
    "prepush": "run-s validate",
    "prepublish": "in-publish && run-s validate build || not-in-publish",
    "postpublish": "run-s docs:publish && git push --follow-tags --no-verify"
  },
  "keywords": [
    "nordnet",
    "component",
    "kit",
    "format",
    "react",
    "intl"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "lib"
  ],
  "devEngines": {
    "node": "4.x || 5.x || 6.x",
    "npm": "2.x || 3.x"
  },
  "author": "Andreas Brostr√∂m <andreas.brostrom@nordnet.se>",
  "repository": "nordnet/nordnet-component-kit",
  "bugs": {
    "url": "https://github.com/nordnet/nordnet-component-kit/issues"
  },
  "homepage": "https://nordnet.github.io/nordnet-component-kit",
  "license": "MIT",
  "peerDependencies": {
    "classnames": "2.x",
    "nordnet-ui-kit": ">=0.0.54",
    "react": "15.x",
    "react-dom": "15.x",
    "react-intl": "2.x"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.8.0",
    "cheerio": "^0.22.0",
    "coveralls": "^2.13.0",
    "css-loader": "^0.28.0",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.8.0",
    "eslint": "^4.1.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "git-directory-deploy": "^1.5.1",
    "husky": "^0.14.0",
    "in-publish": "^2.0.0",
    "intl": "^1.2.5",
    "jsdom": "^9.12.0",
    "lodash.camelcase": "^4.3.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "nordnet-ui-kit": "^0.1.1",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.2",
    "postcss-loader": "^2.0.5",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-intl": "^2.2.3",
    "react-styleguidist": "^6.0.0",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "sinon": "^3.0.0",
    "sinon-chai": "^2.9.0",
    "style-loader": "^0.18.0",
    "webpack": "^2.3.3",
    "webpack-bundle-analyzer": "^2.3.0"
  }
}
