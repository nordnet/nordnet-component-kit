{
  "name": "nordnet-component-kit",
  "version": "0.2.8",
  "description": "Custom react components used by Nordnet",
  "main": "dist",
  "scripts": {
    "precommit": "lint-staged",
    "commit": "git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "dev": "run-s start",
    "start": "styleguidist server --colors",
    "clean": "rimraf dist",
    "build": "run-s clean babel",
    "watch": "npm run babel -- --watch",
    "babel": "babel src -d dist --copy-files --ignore .md",
    "test": "mocha --opts test/mocha.opts",
    "tdd": "mocha --opts test/mocha.opts --watch --watch-extensions js,jsx",
    "coverage": "cross-env NODE_ENV=coverage nyc npm test",
    "precoveralls": "run-s coverage",
    "coveralls": "coveralls < coverage/lcov.info",
    "lint": "eslint src test --ext .js,.jsx",
    "validate": "run-p lint test",
    "prepush": "run-s validate",
    "build:docs": "rimraf documentation/build && styleguidist build",
    "predocs-publish": "run-s build:docs",
    "docs-publish": "gh-pages -d documentation/dist --repo https://$GH_TOKEN@github.com/nordnet/nordnet-component-kit.git --silent",
    "prepublish": "in-publish && run-s validate build || not-in-publish",
    "semantic-release": "semantic-release",
    "format": "prettier --write '{e2e-test,src}/**/*.{js,jsx}'"
  },
  "keywords": [
    "nordnet",
    "component",
    "kit",
    "format",
    "react",
    "intl"
  ],
  "files": [
    "dist",
    "src"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@nordnet/cz-conventional-changelog"
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "include": [
      "src/*"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "all": true,
    "instrument": false,
    "sourceMap": false,
    "reporter": [
      "lcov",
      "text-summary",
      "text"
    ]
  },
  "author": "Andreas BrostrÃ¶m <andreas.brostrom@nordnet.se>",
  "repository": {
    "type": "git",
    "url": "https://github.com/nordnet/nordnet-component-kit"
  },
  "bugs": {
    "url": "https://github.com/nordnet/nordnet-component-kit/issues"
  },
  "homepage": "https://nordnet.github.io/nordnet-component-kit",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@formatjs/intl-datetimeformat": "^2.0.2",
    "@formatjs/intl-getcanonicallocales": "^1.2.10",
    "@formatjs/intl-locale": "^2.1.12",
    "@formatjs/intl-numberformat": "^5.0.2",
    "@formatjs/intl-pluralrules": "^3.0.2",
    "@formatjs/intl-relativetimeformat": "^6.0.2",
    "@nordnet/cz-conventional-changelog": "^1.1.0",
    "@nordnet/prettier-config": "^1.0.0",
    "@webpack-blocks/babel6": "^0.4.1",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "in-publish": "^2.0.0",
    "jsdom": "^11.5.1",
    "lint-staged": "^6.0.0",
    "mocha": "^5.0.0",
    "nordnet-ui-kit": "^1.26.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2",
    "prop-types": "^15.6.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-intl": "^5.0.2",
    "react-jss": "^7.2.0",
    "react-styleguidist": "^6.2.0",
    "react-test-renderer": "^16.13.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^12.2.2",
    "sinon": "^4.1.6"
  },
  "dependencies": {
    "@formatjs/intl-utils": "^3.6.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.5"
  },
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=15",
    "react-intl": ">=5"
  },
  "resolutions": {
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "uglify-es": "3.3.7"
  }
}
