{
  "extends": "eslint-config-nordnet",
  "rules": {
    "new-cap": [2, {"capIsNewExceptions": ["express.Router"]}],
    "no-param-reassign": [2, { "props": false }],
    "react/forbid-prop-types": 0,
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
